<% var memoryConsumes=$.isArray(this)?this: [this] %>
<% $.each(memoryConsumes, function(index, memoryConsume) { %>
<div class="inner-section">
	<div class="section_head">
        <div class="hide_button"></div>
        <div class="section_title">
        	Memory Consume
        </div>
    </div>
	<div class="inner-header">
		<p>
			Info: <span class="value"><%= memoryConsume.info %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			Current Consume: <span class="value"><%= memoryConsume.curconsume %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			Time: <span class="value"><%= memoryConsume.time %>
		</p>
	</div>
	<div class="section_content">
		<% var total= memoryConsume.total %>
		<div class="memory-consume-total">
			<p class="memory-consume-total-title">Total</p>
			<div class="section_content">
				<% var memoryConsumeSections = $.isArray(total.section)?total.section: [total.section] %>
				<% $.each(memoryConsumeSections, function(sectionIndex, memoryConsumeSection) { %>
				<div class="memory-consume-section">
						<div class="inner-header">
							<p>
								Section Total: <span class="value"><%= memoryConsumeSection.total %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
								Section Count: <span class="value"><%= memoryConsumeSection.count %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
								Count Percentage: <span class="value"><%= memoryConsumeSection.percentage %>
							</p>
						</div>
						<div>
							<%== can.view.render("ejs/stack.ejs", memoryConsumeSection.stack) %>
						</div>
				</div>
				<% }) %>
			</div>
			<div class="memory-consume-section-total-pie" index="<%= index %>"></div>
		</div>
		<% var count = memoryConsume.count %>
		<div class="memory-consume-count">
			<p class="memory-consume-count-title">Count</p>
			<div class="section_content">
				<% var memoryConsumeSections = $.isArray(count.section)?count.section: [count.section] %>
				<% $.each(memoryConsumeSections, function(sectionIndex, memoryConsumeSection) { %>
				<div class="memory-consume-section">
						<div class="inner-header">
							<p>
								Section Total: <span class="value"><%= memoryConsumeSection.total %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
								Section Count: <span class="value"><%= memoryConsumeSection.count %></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
								count Percentage: <span class="value"><%= memoryConsumeSection.percentage %>
							</p>
						</div>
						<div>
							<%== can.view.render("ejs/stack.ejs", memoryConsumeSection.stack) %>
						</div>
				</div>
				<% }) %>
			</div>
			<div class="memory-consume-section-count-pie" index="<%= index %>"></div>
		</div>
	</div>
</div>
<% }) %>